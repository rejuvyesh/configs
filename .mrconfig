[src/misc/gitignore]
checkout = git clone 'https://github.com/github/gitignore.git' 'gitignore'

[src/julia/julia]
checkout = git clone 'https://github.com/JuliaLang/julia.git' 'julia'

[src/scripts]
checkout = git clone 'https://rejuvyesh@github.com/rejuvyesh/Scripts.git' 'scripts'

[/home/rejuvyesh]
checkout = git clone 'https://rejuvyesh@github.com/rejuvyesh/configs.git' 'rejuvyesh'

[.zsh/zsh-syntax-highlighting]
checkout = git clone 'git://github.com/zsh-users/zsh-syntax-highlighting.git' 'zsh-syntax-highlighting'

[.rbenv]
checkout = git clone 'https://github.com/sstephenson/rbenv.git' '.rbenv'

[.rbenv/plugins/ruby-build]
checkout = git clone 'https://github.com/sstephenson/ruby-build.git' 'ruby-build'
